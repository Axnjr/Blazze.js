        "blazze-dev": "index.js",
        "blazze-build": "src/build.js"
    },
    "keywords": [
        "Express",
        "Directory router",
        "API's",
        "Site builder",
        "Typesafe api's",
        "blazze.js",
        "Web server",
        "swc",
        "cli",
        "Rest"
    ],
    "author": "Yakshit Chhipa - Axn",
    "license": "ISC",
    "dependencies": {
        "@inquirer/prompts": "^3.3.0",
        "@swc/core": "^1.3.102",
        "blazze": "^1.1.35-dev",
        "chalk": "^5.3.0",
        "dotenv": "latest",
        "express": "^4.18.2",
        "nodemon": "^3.0.2",
        "ora": "^7.0.1",
        "typescript": "^5.3.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.4"
    },
    "type": "module"
}
