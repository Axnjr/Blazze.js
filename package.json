{
    "name": "blazze",
    "version": "1.3.4",
    "description": "The express framework for the web. Write Typesafe API's with no configuration and boiler-plate with dynamic directory based routing and intuitive CLI.",
    "main": "index.js",
    "repository": "Axnjr/Blazze.js",
    "homepage": "blazze.vercel.app",
    "scripts": {
        "build": "node src/build.js",
        "init": "node src/config.js",
        "dev": "node index.js"
    },
    "bin": {
        "blazze-init": "src/config.js",
        "blazze-dev": "index.js",
        "blazze-build": "src/build.js"
    },
    "keywords": [
        "Express JS",
        "CLI",
        "typescript",
        "Typesafe",
        "Nodejs",
        "javascript",
        "API Request Caching",
        "Directory router",
        "REST APIS",
        "Site builder",
        "Typesafe APIS",
        "blazze.js",
        "Web server",
        "swc"
    ],
    "author": "Yakshit Chhipa - Axn",
    "license": "MIT",
    "dependencies": {
        "@inquirer/prompts": "^3.3.0",
        "@swc/core": "^1.3.102",
        "blazze": "^1.1.35-dev",
        "chalk": "^5.3.0",
        "dotenv": "latest",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "nodemon": "^3.0.2",
        "ora": "^7.0.1",
        "typescript": "^5.3.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.4",
        "@types/nodemon": "^1.19.6"
    },
    "type": "module"
}
